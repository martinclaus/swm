&model_nl
  r = 0.,                ! linear bottom friction parameter
  k = 0.,             ! quadratic bottom friction parameter
  gamma_new = 0.,  ! Newtonian cooling parameter (1/s)
  new_sponge_efolding=8e-2, ! efolding scale of sponge layers
  gamma_new_sponge=1.,   ! maximum damping of sponge layer (1/s)
  Ah = 3e2,               ! horizontal eddy viscosity (m^2/s)
  run_length = 2e4, ! length of run in seconds
  dt = 2e1,      ! time step (s)
  meant_out=2.628e5    ! Time span in seconds for each mean step written to disk. Default is 1/12 of a 365 day year.
  file_eta_init=""
  diag_start=0.
  rayleigh_damp_cont=.true.
  rayleigh_damp_mom=.true.
/

&calendar_nl
  model_start="1945-01-01 12:00"
/

&domain_nl
  A = 6371000,          ! radius of the earth (m)
  OMEGA = 7.272205e-05, ! 1/day (1/s)
  RHO0 = 1024,          ! ref. dens. of water (kg/m^3)
  Nx = 601,               ! number of grid points in x-dir.
  Ny = 401,               ! number of grif points in y-dir.
  theta0 = 20.,
  lon_s = -30.,           ! min. lon. of the domain (H-grid)
  lon_e = 30.          ! max. lon. of the domain (H-grid),
  lat_s = -20.     ! min. lat. of the domain (H-grid),
  lat_e = 20.         ! max. lat. of the domain (H-grid)
  in_file_H = "" ! topography input dataset
  H_overwrite=300.
/

