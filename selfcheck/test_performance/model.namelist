&model_nl
  Ah = 3e2,               ! horizontal eddy viscosity (m^2/s)
  run_length = 2e4, ! length of run in seconds
  dt = 2e1,      ! time step (s)
  meant_out=2.628e5    ! Time span in seconds for each mean step written to disk. Default is 1/12 of a 365 day year.
  file_eta_init=""
  diag_start=0.
/

&calendar_nl
  model_start="1945-01-01 12:00"
/

&domain_nl
  A = 6371000,          ! radius of the earth (m)
  OMEGA = 7.272205e-05, ! 1/day (1/s)
  RHO0 = 1024,          ! ref. dens. of water (kg/m^3)
  Nx = 601,               ! number of grid points in x-dir.
  Ny = 401,               ! number of grif points in y-dir.
  theta0 = 20.,
  lon_s = -30.,           ! min. lon. of the domain (H-grid)
  lon_e = 30.          ! max. lon. of the domain (H-grid),
  lat_s = -20.     ! min. lat. of the domain (H-grid),
  lat_e = 20.         ! max. lat. of the domain (H-grid)
  in_file_H = "" ! topography input dataset
  H_overwrite=300.
/

&swm_damping_nl
  term="GAMMA_LIN_ETA"
  type="sponge"
  sl_location="NSEW"
  value=1.
  sl_length=8e-2
  sl_cutoff=15.
/
&swm_damping_nl
  term="GAMMA_LIN_U"
  type="sponge"
  sl_location="NSEW"
  value=1.
  sl_length=8e-2
  sl_cutoff=15.
/
&swm_damping_nl
  term="GAMMA_LIN_V"
  type="sponge"
  sl_location="NSEW"
  value=1.
  sl_length=8e-2
  sl_cutoff=15.
/

&output_nl
  oprefix = "output/new_",  ! output prefix used to specify directory
  osuffix = "",  ! suffix to name model run
/

!&diag_nl filename="taux.nc", varname="f_x", type="S", frequency=10 /
!&diag_nl filename="u.nc", varname="u", type="S", frequency=10 /
!&diag_nl filename="eta.nc", varname="eta", type="S", frequency=10 /
