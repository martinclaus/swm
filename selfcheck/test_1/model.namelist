&model_nl
  Ah = 5e3,               ! horizontal eddy viscosity (m^2/s)
  run_length = 8.67e5, ! length of run in seconds
  dt = 25,      ! time step (s)
  meant_out=2.628e6    ! Time span in seconds for each mean step written to disk. Default is 1/12 of a 365 day year.
  diag_start=0.
/

&swm_nl
  file_eta_init="input/eta_init_gauss.nc"
/

&calendar_nl
  model_start="1945-01-01 12:00"
/

&domain_nl
  A = 6371000,          ! radius of the earth (m)
  OMEGA = 7.272205e-05, ! 1/day (1/s)
  RHO0 = 1024,          ! ref. dens. of water (kg/m^3)
  Nx = 61,               ! number of grid points in x-dir.
  Ny = 41,               ! number of grif points in y-dir.
  theta0 = 20.,
  lon_s = -30.,           ! min. lon. of the domain (H-grid)
  lon_e = 0.          ! max. lon. of the domain (H-grid),
  lat_s = -10.     ! min. lat. of the domain (H-grid),
  lat_e = 10.         ! max. lat. of the domain (H-grid)
  in_file_H = "input/H_sc.nc" ! topography input dataset
  H_overwrite=3.
/

&swm_timestep_nl
  filename="input/bs_sc.nc"
  varname="PSI"
  chunksize=1
  AB_Chi = .1
/

!Forcing Files
&swm_forcing_nl filename="input/taux_sc.nc", varname="TAUX", forcingtype="W", component="Z" /
&swm_forcing_nl filename="input/tauy_sc.nc", varname="TAUY", forcingtype="W", component="M" /

&swm_damping_nl type="sponge" term="GAMMA_LIN_ETA" sl_location="NSEW" value=1. sl_length=8e-2 sl_cutoff=15. /
&swm_damping_nl type="sponge" term="GAMMA_LIN_U"   sl_location="NSEW" value=1. sl_length=8e-2 sl_cutoff=15. /
&swm_damping_nl type="sponge" term="GAMMA_LIN_V"   sl_location="NSEW" value=1. sl_length=8e-2 sl_cutoff=15. /


&dynFromFile
!  FileName_u="input/u_dff_sc.nc",
!  FileName_v="input/v_dff_sc.nc",
!  FileName_eta="input/eta_dff_sc.nc", ! Filenames of input files of dynamical variables
!  FileName_psi="input/bs_sc.nc",
!  DFF_Nt_chunksize=57 ! Number of timesteps to read at once
/
&tracer_nl
  TRC_C1_A = 1e2     ! horizontal tracer diffusivity
  TRC_file_C0 = "input/C0_sc.nc"
  TRC_file_relax = "input/Crelax_sc.nc"
/
&output_nl
  oprefix = "output/new_",  ! output prefix used to specify directory
  osuffix = "",  ! suffix to name model run
/

&diag_nl filename="u_out.nc", varname="u", type="S", frequency=10 /
&diag_nl filename="v_out.nc", varname="v", type="snapshot", period=2.628e6, frequency=10 /
&diag_nl filename="eta_out.nc", varname="eta", type="S", frequency=10 /
&diag_nl filename="psi_out.nc", varname="psi", type="S", frequency=10 /
