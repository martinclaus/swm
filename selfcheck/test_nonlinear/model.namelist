&model_nl
  G = 4e-2
  r = 0., !2.8935185185185184e-06,  ! linear bottom friction parameter
  gamma_new = 0., !2.3148148148148148e-08,  ! Newtonian cooling parameter (1/s)
  new_sponge_efolding=8e-2, ! efolding scale of sponge layers
  gamma_new_sponge=1.,   ! maximum damping of sponge layer (1/s)
  Ah = 5e3,               ! horizontal eddy viscosity (m^2/s)
  run_length = 5.184e7, ! length of run in seconds
  dt = 3.5e3  ! time step (s)
  rayleigh_damp_cont=.true.
  rayleigh_damp_mom=.true.
/

&calendar_nl
  model_start="2000-01-01 00:00"
/

&domain_nl
  A = 6371000,          ! radius of the earth (m)
  OMEGA = 7.272205e-05, ! 1/day (1/s)
  RHO0 = 1024,          ! ref. dens. of water (kg/m^3)
  Nx = 201,             ! number of grid points in x-dir.
  Ny = 101,             ! number of grif points in y-dir.
  lon_s = 0.,           ! min. lon. of the domain (H-grid)
  lon_e = 100.          ! max. lon. of the domain (H-grid),
  lat_s = -25.     ! min. lat. of the domain (H-grid),
  lat_e = 25.         ! max. lat. of the domain (H-grid)
  H_overwrite=125.
/

&swm_timestep_nl
  AB_Chi = .1
/

&swm_forcing_nl filename="input/taux.nc", varname="TAUX", forcingtype="W", component="Z" /

&output_nl
  oprefix = "output/new_",  ! output prefix used to specify directory
  osuffix = "",  ! suffix to name model run
/

